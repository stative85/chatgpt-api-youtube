{
  "name": "neuro-engine",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "pnpm exec playwright test",
    "clean": "turbo run clean",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.0.4",
    "@eslint/js": "8.57.0",
    "@playwright/test": "1.44.1",
    "@types/node": "20.12.7",
    "eslint": "8.57.0",
    "eslint-config-next": "15.0.0-canary.22",
    "husky": "9.1.3",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.5",
    "turbo": "2.1.0",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vitest": "1.5.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "pnpm exec eslint"
  },
  "pnpm": {
    "packages": [
      "apps/*",
      "packages/*",
      "docs"
    ]
  }
}
